:components:
A := 200
B := 0x01
C := 63000
D := "what a nice string it is today!":0x00
E := "I dont have a null at the end"
F: byte := b01010101
G: byte{4} := 0x01:0x03:0x04:0x02
H: byte := 0x01|0x02
I: byte{2} := (0x01:0x04)|(0x52:0x50)
J: byte{2} := (0x50|0xf0):(0x30:40)
K := 255

root := A:B:C:D:E:F:G:H:I:J:K
